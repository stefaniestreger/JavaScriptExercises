<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">	
    <title>Convert from Fahrenheit to Centigrade</title>
    <script>
        "use strict";

        let tempFarhenheit = parseInt(prompt("Enter Fahrenheit temperature"));
        let tempCelsius = parseInt((tempFarhenheit - 32) * 5 / 9);

        alert("Fahrenheit temperature is " + tempFarhenheit + "\n" + "\n" + "Celsius temperature is " + tempCelsius);
       
    </script>
</head>
<body>
    <main>
        <h1>Thanks for using the temperature converter</h1>
    </main>
</body>
</html>
